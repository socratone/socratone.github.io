{
  "name": "socratone.github.io",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn blogs-metadata && next dev",
    "build": "next build",
    "start": "npx serve@latest out",
    "lint": "next lint",
    "blogs-metadata": "ts-node --project tsconfig.node.json ./scripts/blogs-metadata.ts ./generated",
    "resume-pdf": "env-cmd -f .env.resume next build && ts-node --project tsconfig.node.json ./scripts/resume-pdf.ts && open resume.pdf",
    "sitemap": "ts-node --project tsconfig.node.json ./scripts/sitemap.ts ./out",
    "optimize-thumbnail-image": "yarn optimize-blog-thumbnail-image && yarn optimize-lifehack-thumbnail-image",
    "optimize-blog-thumbnail-image": "ts-node --project tsconfig.node.json ./scripts/optimize-image.ts ./components/BlogListItem/images/blogs ./public/images/blog/thumbnail --width 96 --width 192",
    "optimize-lifehack-thumbnail-image": "ts-node --project tsconfig.node.json ./scripts/optimize-image.ts ./components/BlogListItem/images/lifehacks ./public/images/lifehack/thumbnail --width 96 --width 192",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@mui/icons-material": "^5.14.9",
    "@mui/material": "^5.13.3",
    "@mui/material-nextjs": "^5.15.6",
    "@next/mdx": "^14.2.3",
    "@next/third-parties": "^14.0.4",
    "dayjs": "^1.11.10",
    "fuse.js": "^7.0.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.9.0",
    "jsdom": "^23.0.1",
    "marked": "^9.1.2",
    "next": "^14.0.4",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.16.0",
    "react-use": "^17.5.0",
    "sharp": "^0.33.4",
    "socratone-utils": "^0.0.6"
  },
  "devDependencies": {
    "@styled/typescript-styled-plugin": "^1.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.6",
    "@types/mdx": "^2.0.13",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "^14.0.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "puppeteer": "^21.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "xmlbuilder": "^15.1.1"
  }
}
